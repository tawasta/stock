<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <record id="stock_move_create_manufacturing_order_message_wizard" model="ir.ui.view">
        <field name="name">stock.move.create.manufacturing.order.message.form</field>
        <field name="model">stock.move.create.manufacturing.order.message</field>
        <field name="arch" type="xml">
            <form id="create_manufacturing_order_message_form">
                <group>
                    <group>
                        <div id="mo_message">
                            <field  name="mo_message" readonly="1"/>
                        </div>
                    </group>
                    <group>
                        <div id="delivery_message">
                            <field name="delivery_message" readonly="1"/>
                        </div>
                    </group>
                </group>
                <footer>
                    <button special="cancel" string="OK" class="oe_highlight"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
