<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="action_report_product_label" model="ir.actions.report">
        <field name="name">Product label</field>
        <field name="model">stock.picking</field>
        <field name="report_type">qweb-pdf</field>
        <field
            name="report_name"
        >stock_report_label_product.report_product_label</field>
        <field
            name="report_file"
        >stock_report_label_product.report_product_label</field>
        <field name="print_report_name">'Product label_%s' % (object.name)</field>
        <field
            name="paperformat_id"
            ref="stock_report_label_product.paperformat_stock_product_label"
        />
        <field name="binding_model_id" ref="stock.model_stock_picking" />
        <field name="binding_type">report</field>
    </record>
</odoo>
