<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="report_picking" inherit_id="stock.report_picking">
        <th name="th_from" position="attributes">
            <attribute name="t-if">1 == 0</attribute>
        </th>

        <xpath
            expr="//span[@t-esc='ml.location_id.display_name']/.."
            position="attributes"
        >
            <attribute name="t-if">1 == 0</attribute>
        </xpath>
    </template>
</odoo>
