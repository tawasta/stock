<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="report_picking_element_sizes" inherit_id="stock.report_picking">
        <xpath expr="//div[@class='row mt48 mb32']" position="attributes">
            <attribute name="id">stock_picking_informations</attribute>
        </xpath>
    </template>
    <template
        id="report_delivery_document_element_sizes"
        inherit_id="stock.report_delivery_document"
    >
        <xpath expr="//div[@class='row mt32 mb32']" position="attributes">
            <attribute name="id">stock_deliveryslip_informations</attribute>
        </xpath>
    </template>
    <template
        id="assets_stock_report_elements"
        name="Assets to modify element sizes"
        inherit_id="web.report_assets_common"
        active="True"
        priority="1"
    >
        <xpath expr="." position="inside">
            <link
                rel="stylesheet"
                type="text/srcc"
                href="/stock_report_element_sizes/static/src/scss/style.scss"
            />
        </xpath>
    </template>
</odoo>
