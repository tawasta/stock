<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="report_delivery_document" inherit_id="stock.report_delivery_document">
        <xpath expr="//span[@t-field='move.product_uom_qty']" position="attributes">
            <attribute
                name="t-options"
            >{'precision': o.company_id.stock_report_decimal_precision}</attribute>
        </xpath>
    </template>

    <template
        id="stock_report_delivery_has_serial_move_line"
        inherit_id="stock.stock_report_delivery_has_serial_move_line"
    >
        <xpath expr="//span[@t-field='move_line.qty_done']" position="attributes">
            <attribute
                name="t-options"
            >{'precision': o.company_id.stock_report_decimal_precision}</attribute>
        </xpath>
    </template>

    <template
        id="stock_report_delivery_aggregated_move_lines"
        inherit_id="stock.stock_report_delivery_aggregated_move_lines"
    >
        <xpath
            expr='//span[@t-esc="aggregated_lines[line][&apos;qty_done&apos;]"]'
            position="attributes"
        >
            <attribute
                name="t-options"
            >{'widget': 'float', 'decimal_precision': 'Company Precision'}</attribute>
        </xpath>
    </template>
</odoo>
