<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_stock_picking_mass_action_form" model="ir.ui.view">
        <field name="model">stock.picking.mass.action</field>
        <field name="inherit_id" ref="stock_picking_mass_action.view_stock_picking_mass_action_form"/>
        <field name="arch" type="xml">
            <field name="transfer" position="after">
                <field name="unreserve"/>
            </field>
        </field>
    </record>
    <!-- Unreserve Action -->
    <record id="action_unreserve" model="ir.actions.act_window">
        <field name="name">Unreserve</field>
        <field name="res_model">stock.picking.mass.action</field>
        <field name="view_type">form</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
        <field name="context">{'unreserve': 1}</field>
        <field name="binding_model_id" ref="stock.model_stock_picking"/>
    </record>
</odoo>
