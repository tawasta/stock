<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_remove_picking_operations_print_button" model="ir.cron">
            <field name="name">Remove Picking Operations print button</field>
            <field name="model_id" ref="base.model_ir_actions_report" />
            <field name="state">code</field>
            <field
                name="code"
            >env.ref('stock.action_report_picking').unlink_action()</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number" eval="1" />
            <field name="interval_type">days</field>
            <field name="numbercall" eval="-1" />
            <field name="priority" eval="5" />
            <field name="doall" eval="0" />
            <field name="active" eval="1" />
        </record>
    </data>
</odoo>
