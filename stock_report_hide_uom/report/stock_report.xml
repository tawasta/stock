<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="report_delivery_document" inherit_id="stock.report_delivery_document">
        <span t-field="move.product_uom" position="attributes">
            <attribute name="t-if">1 == 0</attribute>
        </span>

        <span t-field="bo_line.product_uom" position="attributes">
            <attribute name="t-if">1 == 0</attribute>
        </span>
    </template>

    <template
        id="stock_report_delivery_has_serial_move_line"
        inherit_id="stock.stock_report_delivery_has_serial_move_line"
    >
        <span t-field="move_line.product_uom_id" position="attributes">
            <attribute name="t-if">1 == 0</attribute>
        </span>
    </template>

    <template
        id="stock_report_delivery_aggregated_move_lines"
        inherit_id="stock.stock_report_delivery_aggregated_move_lines"
    >
        <span t-esc="aggregated_lines[line]['product_uom']" position="attributes">
            <attribute name="t-if">1 == 0</attribute>
        </span>
    </template>
</odoo>
