<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="report_picking" inherit_id="stock.report_picking">
        <th name="th_barcode" position="before">
            <th id="th_product_vendor_name" t-if="o.picking_type_id.code == 'incoming'">
                <strong>Vendor product name</strong>
            </th>

            <th id="th_product_vendor_code" t-if="o.picking_type_id.code == 'incoming'">
                <strong>Vendor product code</strong>
            </th>
        </th>

        <xpath
            expr="//span[@t-esc='ml.lot_id.name or ml.lot_name']/.."
            position="before"
        >
            <td id="td_product_vendor_name" t-if="o.picking_type_id.code == 'incoming'">
                <span t-field="ml.product_id.primary_vendor_id" />
            </td>

            <td id="td_product_vendor_code" t-if="o.picking_type_id.code == 'incoming'">
                <span t-field="ml.product_id.primary_vendor_code" />
            </td>
        </xpath>
    </template>
</odoo>
