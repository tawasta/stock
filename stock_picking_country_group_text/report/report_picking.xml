<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="stock_picking_country_group_report" inherit_id="stock.report_picking">
        <xpath expr="//p[@t-field='o.note']" position="after">
            <t
                t-foreach="o.partner_id.country_id.country_group_ids"
                t-as="country_group"
            >
                <br />
                <span t-field="country_group.stock_picking_text" />
            </t>
        </xpath>
    </template>
</odoo>
