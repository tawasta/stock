<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <template id="sale_order_customer_order_date_report_saleorder_document" inherit_id="sale.report_saleorder_document">
        <xpath expr="//p[@t-field='doc.note']" position="after">
            <t t-foreach="doc.partner_id.country_id.country_group_ids" t-as="country_group">
                <br/>
                <t t-esc="country_group.sale_order_text"></t>
            </t>
        </xpath>
    </template>
</odoo>
