<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_picking" inherit_id="stock.report_picking">
        <xpath expr="//t[@t-foreach='move.move_line_ids.sorted(key=lambda ml: ml.location_id.id)']//tr//td[1]" position="replace">
            <td>
                <span t-field="ml.product_id.default_code"/>
            </td>
        </xpath>
    </template>
</odoo>
