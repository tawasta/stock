<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <record id="view_immediate_transfer" model="ir.ui.view">
        <field name="name">stock.immediate.transfer.run.funtion.as.sudo</field>
        <field name="model">stock.immediate.transfer</field>
        <field name="inherit_id" ref="stock.view_immediate_transfer"/>
        <field name="arch" type="xml">
            <button name="process" position="after">
                <button name="sudo_process" string="_Apply" type="object" class="btn-primary"/>
            </button>
            <button name="process" position="attributes">
                <attribute name="invisible">1</attribute>
            </button>
        </field>
    </record>
</odoo>

